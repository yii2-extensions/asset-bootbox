{
    "name": "yii2-extensions/asset-bootbox",
    "type": "yii2-extension",
    "description": "BootBox for Yii Framework.",
    "keywords": [
        "yii2",
        "asset",
        "bootbox"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "yiisoft/yii2": "^2.2",
        "npm-asset/bootbox": "@dev"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.6",
        "phpunit/phpunit": "^10.2",
        "yii2-extensions/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "yii\\assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii\\extensions\\assets\\tests\\": "tests"
        }
	},
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "yiisoft/yii2-composer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
